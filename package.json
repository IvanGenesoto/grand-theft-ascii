{
  "name": "anarch-city",
  "version": "1.0.0",
  "description": "A massively multiplayer persistent online world for people who want to experience a simulated anarchic society.",
  "main": "build/server/index.js",
  "dependencies": {
    "express": "^4.15.2",
    "performance-now": "^2.1.0",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-object-entries-and-values-to-object-keys": "^1.0.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "htmlhint": "^0.12.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "build": "babel src -vd -d build --copy-files",
    "bundle": "browserify build/client/index.js -v --transform babelify -o build/server/public/main.js",
    "prestart": "npm run build && npm run bundle",
    "start": "node .",
    "watch:server": "nodemon .",
    "watch:client": "watchify build/client/index.js -vd --transform babelify -o build/server/public/main.js",
    "watch": "npm run prestart && run-p watch:client watch:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanGenesoto/anarch-city.git"
  },
  "bugs": {
    "url": "https://github.com/IvanGenesoto/anarch-city/issues"
  },
  "homepage": "https://github.com/IvanGenesoto/anarch-city#readme"
}
